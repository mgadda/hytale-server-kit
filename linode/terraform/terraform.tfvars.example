linode_token   = "your-linode-api-token"
ssh_public_key = "ssh-ed25519 AAAA... your-key"
root_password  = "a-secure-root-password"

# Optional: change region (default: us-west)
# region = "us-east"

# Optional: change server label (default: hytale-server)
# server_label = "my-hytale-server"
